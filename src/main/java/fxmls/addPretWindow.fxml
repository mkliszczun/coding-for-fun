<BorderPane prefHeight="305.0" prefWidth="456.0" style="-fx-background-color: #162e46;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AddBarWindow">
   <bottom>
