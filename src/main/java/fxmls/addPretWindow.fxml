               </children></VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="12.0">
               <children>
                  <Label text="Ilość" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <Label text="Żebro" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
               </children></VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" spacing="6.0">
               <children>
                  <TextField fx:id="count" text="0" />
                  <CheckBox fx:id="zebro" mnemonicParsing="false" prefHeight="24.0" prefWidth="24.0" />
               </children></VBox>
         </children>
